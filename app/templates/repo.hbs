<h1><a href="{{unbound repo.html_url}}">{{repo.id}}</a>
<h4>{{repo.description}}</h3>
<h3>Clippings</h4>
<ul>
    {{#each clipping in clippings}}
        {{#with clipping}}
            <li><a href="{{unbound clipping.url}}">{{clipping.url}}</a></li>
        {{/with}}
    {{else}}
        <li>This library has no clippings. Clip the first!</li>
    {{/each}}
</ul>
